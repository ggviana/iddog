{
  "name": "iddog",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "axios": "0.18.0",
    "normalize.css": "8.0.0",
    "prop-types": "15.6.2",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-router-dom": "4.3.1",
    "reworm": "2.0.2"
  },
  "devDependencies": {
    "enzyme": "3.4.4",
    "enzyme-adapter-react-16": "1.2.0",
    "gh-pages": "2.0.1",
    "jest-localstorage-mock": "2.2.0",
    "react-scripts": "1.1.5",
    "standard": "11.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "prebuild": "npm run test:build && npm run lint",
    "build": "react-scripts build",
